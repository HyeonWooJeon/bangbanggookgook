@charset "UTF-8";

.dataRenewalWrapper {
	width : 28%;
	max-height : 930px;
	background : white;
	margin : auto;
	box-shadow : 8px 8px 8px black;
	padding : 10px 10px 15px 10px;
	border-radius : 5px;
}

.btnArea1 {
	display : flex;
	flex-direction : column;
}

.btn-secondary-shapeFix {
	height : 60px;
	font-size : 19px !important;
	box-shadow : 3px 5px 5px darkgray;
/* 	font-weight : bold !important; */
}	


label {
	size : 17px;
	font-weight : bold;
}

.modal-overlay{
	background : red;
	display: none;
	position : absolute;
	top : 0;
	left : 0;
	width : 100%;
	height : 100%;
	z-index : 999;
	background: rgba(255, 255, 255, 0.25);
	backdrop-filter: blur(1.5px);
}

.modalArea {
	position : relative;
	top : 25%;
	background : #FAFAFA;
	width : 15%;
	height : 23%;
	margin : auto;
	border-radius : 7px;
}
