* {
	margin: 0px;
	padding: 0px;
	box-sizing: border-box;
}

.wrap {
	margin: auto;
	justify-content: center;
	align-items: center;
	margin-left: 25%;
}

.title {
	margin-top: 5%;
	height: 30px;
}

.title img {
	width: 30px;
	height: 30px;
}

.title span {
	font-size: 20px;
	color: #747474;
}

.subTitle span {
	font-size: 40px;
	font-weight: bold;
}
/*제목 끝 */
.info {
	    margin: 2% 0 5% 20%;
	/* width: 550px; */
	height: 400px;
}

.table1 {
	margin:0 0 0 20%;
	width: 800px;
	border-color: #BDBDBD;
}

.table1 thead {
	font-size: 50px;
	border-bottom: 1px solid black;


}
.table1 label {
display: inline-block;
  padding: .5em .75em;
  color: #999;
  font-size: inherit;
  line-height: normal;
  vertical-align: middle;
  background-color: #fdfdfd;
  cursor: pointer;
      border: 1px solid black;
/*  border-bottom-color: #e2e2e2; */
  border-radius: .25em; 
}
.table1 input[type="file"] {  /* 파일 필드 숨기기 */
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip:rect(0,0,0,0);
  border: 0;
}
.table1 img{
	width: 100px;
	height : 100px;
}

.table1 .button{
	display: inline-block;
  padding: .5em .75em;
  color: #999;
  font-size: inherit;
  line-height: normal;
  vertical-align: middle;
  background-color: #fdfdfd;
  cursor: pointer;
      border: 1px solid black;
/*  border-bottom-color: #e2e2e2; */
  border-radius: .25em; 
}
table .td {
	width: 90px;
	height: 50px;
	text-align: left;
	border-right: 2px solid black;
}

table .td1 {
	width: 120px;
	text-align: left;
	height: 50px;
	border-right: 2px solid black;
	margin-left: 10px;
}
.wrap hr{
	    width: 900px;
    margin: 2% 0 2% 15%;
}
.button {
	cursor: pointer;
	background-color: #fff;
	width: 100px;
	border: 1px solid black;
}

.submitButton {
	background-color: black;
	border-radius: 4px;
	border-style: none;
	box-sizing: border-box;
	color: #fff;
	cursor: pointer;
	font-size: 16px;
	font-weight: 700;
	max-width: none;
	height: 40px;
	min-width: 10px;
	outline: none;
	overflow: hidden;
	padding: 9px 20px 8px;
	position: relative;
	text-align: center;
	touch-action: manipulation;
	width: 150px;
}

.submitButton:hover {
	color: black;
	border-radius: 4px;
	border-style: none;
	box-sizing: border-box;
	background-color: #fff;
	cursor: pointer;
	font-size: 16px;
	font-weight: 700;
	max-width: none;
	height: 40px;
	min-width: 10px;
	outline: none;
	overflow: hidden;
	padding: 9px 20px 8px;
	position: relative;
	text-align: center;
	touch-action: manipulation;
	width: 150px;
}

.inputBox {
	width: 200px;
	height: 32px;
	font-size: 15px;
	border-color: #EAEAEA;
	border-radius: 5px;
	outline: 1px;
	padding-left: 10px;
	margin-left: 10px;
}

.inputBox:focus {
	outline: none !important;
	border-color: #EAEAEA;
	box-shadow: 0 0 10px #EAEAEA;
}

input[type=submit] {
	margin: 20px 0px 10px 0px;
}
input[type=button] {
    margin: 10px 0px 10px 10px;
}
input[type=radio] {
	margin-left: 10px;
}