*{
	margin:0px;
	padding:0px;
	box-sizing: border-box;
}
.wrap {
	margin: auto;
	justify-content: center;
	align-items: center;
	margin-left: 25%;
}
.title{
	margin-top:5%;
	height:30px;
}
.title img{
	 width: 30px;
  height: 30px;
}
.title span{
	font-size: 20px;
	color: #747474;
}
.subTitle span{
	font-size:40px;
	font-weight: bold;
}
/*제목 끝 */
.content{
	margin:auto;
}

.content table{
	margin:5% 10% 10% 25%;
}
table .td{
	width: 120px;
	text-align: left;
}

.submitButton {
	background-color: black;
	border-radius: 4px;
	border-style: none;
	box-sizing: border-box;
	color: #fff;
	cursor: pointer;
	font-size: 16px;
	font-weight: 700;
	
	max-width: none;
	height: 40px;
	min-width: 10px;
	outline: none;
	overflow: hidden;
	padding: 9px 20px 8px;
	position: relative;
	text-align: center;
	touch-action: manipulation;
	width: 150px;
}

.submitButton:hover {
	color: black;
	border-radius: 4px;
	border-style: none;
	box-sizing: border-box;
	background-color: #fff;
	cursor: pointer;
	font-size: 16px;
	font-weight: 700;
/* 	margin: 20px 0 0 0; */
	max-width: none;
	height: 40px;
	min-width: 10px;
	outline: none;
	overflow: hidden;
	padding: 9px 20px 8px;
	position: relative;
	text-align: center;
	touch-action: manipulation;
	width: 150px;
}
.inputBox {
	width: 200px;
	height: 32px;
	font-size: 15px;
	border-color: #EAEAEA;
	border-radius: 5px;
	outline: 1px;
	padding-left: 10px;
	margin-left: 10px;
}

.inputBox:focus {
	outline: none !important;
	border-color: #EAEAEA;
	box-shadow: 0 0 10px #EAEAEA;
}
input[type=submit] {
    margin: 20px 0px 10px 0px;
}
table .td{
	width: 120px;
	height:50px;
	text-align: left;
	border-right : 2px solid black; 
}
